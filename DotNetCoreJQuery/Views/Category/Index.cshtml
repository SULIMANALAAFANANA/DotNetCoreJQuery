<div class="container">
    <h1>Category</h1>
    <hr />
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Main Category</th>
                <th>Date</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody id="tbodyCategory">
           
        </tbody>
    </table>
</div>

<script src="~/js/jquery-3.3.1.min.js"></script>
 <script type="text/javascript">
     $(document).ready(function () {
         fnCountryList();
     });

     function fnCountryList() {
         var CategoryData = $('#tbodyCategory');
         CategoryData.empty();
         $.ajax({
             type: "Post",
             url: "@Url.Action("GetAllcategory", "Category")",
             dataType: "json",
             success: function (result) {
                 console.log(result);
                 
                 $.each(result.categoryList, function (index, raj) {
                     CategoryData.append('
                         < tr >
                         <td>${raj.id}</td>
                         <td>${raj.name}</td>
                         <td>${raj.mainCategory}</td>
                         <td>${raj.creationData}</td>
                         <td>${raj.cId}</td>
                         </tr>

                         ');


                 });
                 fnCitiList();
             },
             error: function (req, status, error) {

             }
         });
     }
 </script>
